{
    "title": "{{ entity.title }}",
    "album": "{{ entity.album }}",
    "track": "{{ entity.track }}",
    "time": "{{ entity.time }}",
    "year": "{{ entity.year }}",
    "bitrate": "{{ entity.bitrate }}",
    "mode": "{{ entity.mode }}",
    "links": {
        "self": "{{ url('webservice.song', { songId: entity.attributes.id }) }}",
        "preview": "{{ entity.art }}",
        "albums" : "{{ url('webservice.album', { albumId: entity.album.attributes.id }) }}",
        "artists" : "{{ url('webservice.artist', { artistId: entity.artist.attributes.id }) }}",
        "media" : "{{ url('webservice.song.streamUri', { songId: entity.attributes.id }) }}"
    }
}
